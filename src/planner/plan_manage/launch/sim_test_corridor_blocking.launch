<launch>
    <arg name="map_size_x" value="40.0"/>
    <arg name="map_size_y" value="6.0"/>
    <arg name="map_size_z" value="3.0"/>
    <arg name="odom_topic" value="visual_slam/odom" />

    <include file="$(find fapp_planner)/launch/include/run_in_test.xml">
        <arg name="drone_id" value="0"/>
        <arg name="init_x" value="-15.0"/>
        <arg name="init_y" value="0.0"/>
        <arg name="init_z" value="1.0"/>
        <arg name="map_size_x" value="$(arg map_size_x)"/>
        <arg name="map_size_y" value="$(arg map_size_y)"/>
        <arg name="map_size_z" value="$(arg map_size_z)"/>
        <arg name="odom_topic" value="$(arg odom_topic)"/>
        <arg name="flight_type" value="1"/>
        <!-- Target: the UAV should attempt to reach the end of corridor -->
        <!-- The blocking obstacles will require adaptive re-planning -->
        <!-- <arg name="point_num" default="1"/>
        <arg name="target0_x" default="15.0"/>
        <arg name="target0_y" default="0.0"/>
        <arg name="target0_z" default="1.0"/> -->
    </include>
</launch>
